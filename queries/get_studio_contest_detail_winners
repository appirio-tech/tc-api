SELECT 
    s.submission_id
  , c.handle AS submitter
  , pr.submit_timestamp AS submission_time
  , pr.placed AS rank
  , pr.final_points AS points
FROM 
   project_result pr
 , submission s
 , coder c
WHERE pr.project_id = @ct@
AND pr.submit_ind = 1
AND s.project_id = pr.project_id
AND s.submitter_id = pr.user_id
AND s.submitter_id = c.coder_id

